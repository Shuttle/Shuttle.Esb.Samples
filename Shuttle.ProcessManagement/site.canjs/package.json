{
  "name": "site.canjs",
  "version": "1.0.0",
  "description": "Shuttle Books",
  "steal": {
    "main": "main.js",
    "directories": {
      "lib": "src"
    },
    "plugins": [
      "steal-stache",
      "steal-css"
    ],
    "meta": {
      "popper.js": {
        "format": "global",
        "deps": [
          "jquery"
        ]
      },
      "bootstrap": {
        "deps": [
          "jquery",
          "popper.js"
        ]
      }
    },
    "paths": {
      "popper.js": "./node_modules/popper.js/dist/umd/popper.js",
      "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js"
    },
    "serviceBaseURL": "http://localhost:8656/api/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "bootstrap": "^4.3.1",
    "can": "^5.28.4",
    "can-define-validate-validatejs": "^1.1.1",
    "can-fixture": "^3.1.6",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "popper.js": "^1.15.0",
    "shuttle-can-api": "^1.0.28",
    "shuttle-canstrap": "^1.1.5",
    "shuttle-guard": "^1.0.0",
    "steal": "^2.2.1",
    "steal-css": "^1.3.2",
    "steal-stache": "^4.1.4",
    "tether": "^1.4.6"
  },
  "devDependencies": {},
  "scripts": {
    "postinstall": "steal-tools bundle --deps --no-minify"
  },
  "author": "",
  "license": "ISC"
}
